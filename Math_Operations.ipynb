    "pi=3.14\n",
    "#area of a circle\n",
    "def area_circle(r):\n",
    "    area=pi*r*r\n",
    "    return area\n",
    "\n",
    "#area of a rectangle\n",
    "def area_rectangle(l,b):\n",
    "    area=l*b\n",
    "    return area\n",
    "\n",
    "#the area of a triangle.\n",
    "def area_triangle(b,h):\n",
    "    area=(b*h)/2\n",
    "    return area\n",
    "\n"
   ]
